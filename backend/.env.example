# Backend/.env.example

# AWS Configuration
AWS_REGION=us-west-2
AWS_PROFILE=default

# Authentication
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Application Settings
APP_URL=https://yourfrontend.com
FROM_EMAIL=noreply@yourdomain.com

# S3 Bucket Names
IMAGES_BUCKET_NAME=your-images-bucket
FRONTEND_BUCKET_NAME=your-frontend-bucket

# Cognito Settings
USER_POOL_NAME=utah-hiking-user-pool

# API Gateway
API_DOMAIN=api.yourdomain.com
API_CERTIFICATE_ARN=arn:aws:acm:region:account:certificate/xxx

# CloudFront
CLOUDFRONT_CERTIFICATE_ARN=arn:aws:acm:us-east-1:account:certificate/xxx
DOMAIN_NAME=yourdomain.com

# Development Settings
STAGE=dev
DEBUG=true
